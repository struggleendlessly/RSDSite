<!-- ========== MAIN CONTENT ========== -->
<main id="content" role="main">
    <!-- Gallery -->
    <div class="container content-space-t-3 content-space-t-lg-5">
        <div class="w-lg-75 text-center mx-lg-auto">
            <!-- Heading -->
            <div class="mb-5 mb-md-10">
                @if (Model.Data != null)
                {
                    <TinyMceEditor 
                        TinyMceId="@(StaticHtmlStrings.TinyAboutUsTitleId)"
                        TinyMceContentFormat="@(StaticStrings.TinyContentFormatHtml)"
                        Model="Model"
                        FuncSave="Save"
                        Key="@(StaticStrings.AboutUsPageDataTitleKey)">
                            <h1 class="display-4">@((MarkupString) Model.Data[StaticStrings.AboutUsPageDataTitleKey])</h1>
                    </TinyMceEditor>
                    <TinyMceEditor 
                        TinyMceId="@(StaticHtmlStrings.TinyAboutUsSubtitleId)"
                        TinyMceContentFormat="@(StaticStrings.TinyContentFormatHtml)"
                        Model="Model"
                        FuncSave="Save"
                        Key="@(StaticStrings.AboutUsPageDataSubtitleKey)">
                            <p class="lead">@((MarkupString) Model.Data[StaticStrings.AboutUsPageDataSubtitleKey])</p>
                    </TinyMceEditor>
                }            
            </div>
            <!-- End Heading -->
        </div>

        <div class="row gx-3">
            <div class="col mb-3">
                <div class="bg-img-start" style="background-image: url(_content/rcl/assets/img/480x320/img17.jpg); height: 15rem;"></div>
            </div>
            <!-- End Col -->

            <div class="col-3 d-none d-md-block mb-3">
                <div class="bg-img-start" style="background-image: url(_content/rcl/assets/img/480x320/img6.jpg); height: 15rem;"></div>
            </div>
            <!-- End Col -->

            <div class="col mb-3">
                <div class="bg-img-start" style="background-image: url(_content/rcl/assets/img/480x320/img14.jpg); height: 15rem;"></div>
            </div>
            <!-- End Col -->

            <div class="w-100"></div>

            <div class="col mb-3 mb-md-0">
                <div class="bg-img-start" style="background-image: url(_content/rcl/assets/img/480x320/img25.jpg); height: 15rem;"></div>
            </div>
            <!-- End Col -->

            <div class="col-4 d-none d-md-block mb-3 mb-md-0">
                <div class="bg-img-start" style="background-image: url(_content/rcl/assets/img/480x320/img16.jpg); height: 15rem;"></div>
            </div>
            <!-- End Col -->

            <div class="col">
                <div class="bg-img-start" style="background-image: url(_content/rcl/assets/img/480x320/img12.jpg); height: 15rem;"></div>
            </div>
            <!-- End Col -->
        </div>
        <!-- End Row -->
    </div>
    <!-- End Gallery -->
    <!-- Feature Stats -->
    <div class="container content-space-2 content-space-lg-3">
        <div class="row justify-content-lg-center">
            <div class="col-sm-4 col-lg-3 mb-7 mb-sm-0">
                <div class="text-center">
                    <h2 class="display-4">7</h2>
                    <p class="small">years in business</p>
                </div>
            </div>
            <!-- End Col -->

            <div class="col-sm-4 col-lg-3 mb-7 mb-sm-0">
                <div class="text-center">
                    <h2 class="display-4">3,5k+</h2>
                    <p class="small">copies sold</p>
                </div>
            </div>
            <!-- End Col -->

            <div class="col-sm-4 col-lg-3">
                <div class="text-center">
                    <h2 class="display-4">85%</h2>
                    <p class="small">happy customers</p>
                </div>
            </div>
            <!-- End Col -->
        </div>
        <!-- End Row -->
    </div>
    <!-- End Feature Stats -->

    <div class="border-top mx-auto" style="max-width: 25rem;"></div>

    <!-- Info -->
    <div class="container content-space-2 content-space-lg-3">
        <div class="row justify-content-lg-between">
            <div class="col-lg-4 mb-5 mb-lg-0">
                @if (Model.Data != null)
                {
                    <TinyMceEditor 
                        TinyMceId="@(StaticHtmlStrings.TinyAboutUsInfoLeftId)"
                        TinyMceContentFormat="@(StaticStrings.TinyContentFormatHtml)"
                        Model="Model"
                        FuncSave="Save"
                        Key="@(StaticStrings.AboutUsPageDataInfoLeftKey)">
                            <h2>@((MarkupString) Model.Data[StaticStrings.AboutUsPageDataInfoLeftKey])</h2>
                    </TinyMceEditor>
                }
            </div>
            <!-- End Col -->

            <div class="col-lg-6">
                @if (Model.Data != null)
                {
                    <TinyMceEditor 
                        TinyMceId="@(StaticHtmlStrings.TinyAboutUsInfoRightFirstId)"
                        TinyMceContentFormat="@(StaticStrings.TinyContentFormatHtml)"
                        Model="Model"
                        FuncSave="Save"
                        Key="@(StaticStrings.AboutUsPageDataInfoRightFirstKey)">
                            <p>@((MarkupString) Model.Data[StaticStrings.AboutUsPageDataInfoRightFirstKey])</p>
                    </TinyMceEditor>
                    <TinyMceEditor 
                        TinyMceId="@(StaticHtmlStrings.TinyAboutUsInfoRightSecondtId)"
                        TinyMceContentFormat="@(StaticStrings.TinyContentFormatHtml)"
                        Model="Model"
                        FuncSave="Save"
                        Key="@(StaticStrings.AboutUsPageDataInfoRightSecondKey)">
                            <p>@((MarkupString) Model.Data[StaticStrings.AboutUsPageDataInfoRightSecondKey])</p>
                    </TinyMceEditor>
                }
            </div>
            <!-- End Col -->
        </div>
        <!-- End Row -->
    </div>
    <!-- End Info -->

    <div class="border-top mx-auto" style="max-width: 25rem;"></div>

    <!-- Team -->
    <div class="container content-space-2 content-space-lg-3">
        <!-- Heading -->
        <div class="w-md-75 w-lg-50 text-center mx-md-auto mb-5 mb-md-9">
