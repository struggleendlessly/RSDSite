<div class="px-2 py-1 text-center container">
    @if (Model.Data != null)
    {
        <HTMLEditor EditorId="@(StaticHtmlStrings.HTMLEditorServicesTitleId)"
                    EditorContentFormat="@(StaticStrings.EditorContentFormatHtml)"
                    Model="Model"
                    FuncSave="Save"
                    Key="@(StaticStrings.ServicesPageDataTitleKey)">
            <h1 class="@StaticHtmlStrings.CSSMainTitle_h1">
                @((MarkupString)Model.Data[StaticStrings.ServicesPageDataTitleKey])
            </h1>
        </HTMLEditor>
    }

    @if (Model.Data != null)
    {
        <ImageEditor EditorId="@StaticHtmlStrings.ImageEditorServicesGalleryImage1Id"
                     Model="Model"
                     FuncSave="Save"
                     Key="@(StaticStrings.ServicesPageDataGalleryImage1Key)">
            <img class="img-fluid rounded my-5" src="@Model.Data[StaticStrings.ServicesPageDataGalleryImage1Key]" alt="Image Description" />
        </ImageEditor>
    }


